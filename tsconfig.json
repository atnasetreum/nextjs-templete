{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components": ["components"],
      "@components/*": ["components/*"],
      "@pages": ["pages"],
      "@pages/*": ["pages/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@models": ["models"],
      "@models/*": ["models/*"],
      "@database": ["database"],
      "@database/*": ["database/*"],
      "@interfaces": ["ts/interfaces"],
      "@interfaces/*": ["ts/interfaces/*"],
      "@enums": ["ts/enums"],
      "@enums/*": ["ts/enums/*"],
      "@types": ["ts/types"],
      "@types/*": ["ts/types/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@themes": ["themes"],
      "@themes/*": ["themes/*"],
      "@store": ["store"],
      "@store/*": ["store/*"],
      "@slices": ["store/slices"],
      "@slices/*": ["store/slices/*"],
      "@hooks": ["hooks"],
      "@hooks/*": ["hooks/*"],
      "@constants": ["constants"],
      "@constants/*": ["constants/*"],
      "@services": ["services"],
      "@services/*": ["services/*"],
      "@contexts": ["contexts"],
      "@contexts/*": ["contexts/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "types.d.ts"],
  "exclude": ["node_modules"]
}
